@{
    @model IEnumerable<MVCJqueryDataTables.Models.Customer>
    ViewBag.Title = "Home Page";
}
<script type="text/javascript">
        @*datatables initialization using vertical scrollbar*@
        $(document).ready(function () {
            $('#tblCust').dataTable({
                "scrollY": "200px",
                "scrollCollapse": true,
                "paging": false
            });
        });
</script>
<div class="jumbotron">
    <h1>JQuery DataTables Demo</h1>
    <p class="lead">Please visit the About and Contact page for more info on the application and author.</p>
    </div>

<div class="row">
    <table id="tblCust">
        <thead>
            <tr>
                <th>Id</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Phone</th>
                <th>Birthday</th>
                <th>City</th>
                <th>Country</th>
                <th>Email</th>
                <th>Region</th>
                <th>Street</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.id</td>
                    <td>@item.first_name</td>
                    <td>@item.last_name</td>
                    <td>@item.phone</td>
                    <td>@item.dob</td>
                    <td>@item.city</td>
                    <td>@item.country</td>
                    <td>@item.email</td>
                    <td>@item.region</td>
                    <td>@item.street</td>
                </tr>
            }
        </tbody>
    </table>
</div>